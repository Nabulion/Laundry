        <Label x:Name="LblName" Content="Name" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextMessage" HorizontalAlignment="Left" Height="209" Margin="332,63,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Data Window" HorizontalAlignment="Left" Margin="332,32,0,0" VerticalAlignment="Top" Width="89"/>

    </Grid>
</Window>
